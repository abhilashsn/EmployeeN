


<table class="table table-condensed">
	<thead>
		<tr>
			<th> Employee Id </th>
			<th> Name </th>
			<th> Date Of Birth </th>
			<th> Gender </th>
			<th> Date of Joining </th>
			<th> Address </th>
			<th> State </th>
		</tr>
	</thead>

	<% Employee.all.each do |employee| %>
	<tbody>
		<tr>
			<td> <%= employee.id %> </td>
			<td> <%= employee.name %> </td>
			<td> <%= employee.date_of_birth %> </td>
			<td> <%= employee.gender %> </td>
			<td> <%= employee.date_of_joining %> </td>
			<td> <%= employee.address %> </td>
			<td> <%= employee.state.name %> </td>
			<td> <%= link_to "edit",edit_employee_path(employee.id)%></td>
			<td> <%= link_to "delete",employee_path(employee.id), method: :delete, data: {confirm: "Are you sure?"} %> </td>  

                       
                     

		</tr>
	</tbody>

<%end%>
</table>

<%= link_to "New Employee",new_employee_path%> 
 <%= link_to "Download CSV", employees_path(format: "csv") %>







