

<%= form_for @pay do |l|%>
  

   <div class="form-group">

   <lable>Start Date :</lable>
   <%= l.text_field :from_date,:id => "paymonth_from_date"%>
   
   </div>



	<div class="form-group" >

    <lable>End Date :</lable>
    <%= l.text_field :to_date,:id => "paymonth_to_date" %>
	
	</div>
    
    <%= l.submit %>
    <%#= link_to 'Create PayMonth', new_pay_month_path, class: 'btn btn-primary'%>

<%end%>